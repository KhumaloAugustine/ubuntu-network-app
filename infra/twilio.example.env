# Example Twilio environment variables for South Africa
# Copy this file to .env in the api/ directory and fill in your actual values

# Twilio Configuration
TWILIO_ACCOUNT_SID=your_account_sid_here
TWILIO_AUTH_TOKEN=your_auth_token_here
TWILIO_PHONE_NUMBER=+27XXXXXXXXX

# Server Configuration
PORT=3000
ALLOWED_ORIGINS=http://localhost:3000,http://localhost:19000,http://10.0.2.2:3000
